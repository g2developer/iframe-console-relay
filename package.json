{
  "name": "iframe-console-relay",
  "version": "0.1.0",
  "description": "Relay console logs from an iframe to the parent window via postMessage.",
  "keywords": [
    "iframe",
    "console",
    "postMessage",
    "logging",
    "browser",
    "relay"
  ],
  "author": "g2developer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/g2developer/iframe-console-relay.git"
  },
  "bugs": {
    "url": "https://github.com/g2developer/iframe-console-relay/issues"
  },
  "homepage": "https://github.com/g2developer/iframe-console-relay#readme",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./iframe": {
      "types": "./dist/iframe.d.ts",
      "import": "./dist/iframe.mjs",
      "require": "./dist/iframe.cjs"
    },
    "./parent": {
      "types": "./dist/parent.d.ts",
      "import": "./dist/parent.mjs",
      "require": "./dist/parent.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && npm run build:umd",
    "build:umd": "rollup -c",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.24.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "tsup": "^8.1.0",
    "typescript": "^5.6.3"
  },
  "unpkg": "dist/index.umd.min.js",
  "jsdelivr": "dist/index.umd.min.js",
  "license": "MIT"
}
